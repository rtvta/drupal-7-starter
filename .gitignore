# Ignore directories generated by Composer
/vendor/
/htdocs/sites/all/drush/
/htdocs/sites/all/modules/contrib/
/htdocs/sites/all/themes/contrib/
/htdocs/sites/all/libraries/

# Ignore Drupal 7 core files.
/htdocs/*
!/htdocs/*/
/htdocs/includes/
/htdocs/misc/
/htdocs/modules/
/htdocs/profiles/README.txt
/htdocs/profiles/minimal/
/htdocs/profiles/standard/
/htdocs/profiles/testing/
/htdocs/scripts/
/htdocs/sites/README.txt
/htdocs/sites/all/*/README.txt
/htdocs/sites/example.sites.php
/htdocs/themes/

# Ignore configuration files that may contain sensitive information.
/htdocs/sites/*/*.settings.php
/htdocs/sites/*/settings*.php

# Ignore paths that contain user-generated content.
/htdocs/sites/*/files
/htdocs/sites/*/private

# Ignore files generated by PhpStorm
/.idea/
